.highlight{background-color:#f8f9fa;border-radius:4px;padding:1rem;margin:1rem 0;overflow-x:auto}.highlight pre{margin:0}.highlight code{background:0 0;padding:0}code{background-color:#f8f9fa;padding:.2rem .4rem;border-radius:4px;font-family:monaco,menlo,ubuntu mono,monospace;font-size:.9em}pre{background-color:#f8f9fa;border-radius:4px;padding:1rem;overflow-x:auto;margin:1rem 0}pre code{background:0 0;padding:0}table{width:100%;border-collapse:collapse;margin:1rem 0}th,td{padding:.75rem;text-align:left;border-bottom:1px solid #e9ecef}th{background-color:#f8f9fa;font-weight:700}img{max-width:100%;height:auto;border-radius:4px;margin:1rem 0}a{color:#007bff;text-decoration:none}a:hover{text-decoration:underline}